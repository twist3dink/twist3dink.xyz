{
    "CodeGPT.apiKey": "CodeGPT Plus",
    "workbench.editor.enablePreviewFromQuickOpen": true,
    "workbench.editor.enablePreviewFromCodeNavigation": true,
    "json.trace.server": "messages",
    "npm.enableRunFromFolder": true,
    "php.validate.executablePath": "",
    "npm-intellisense.scanDevDependencies": true,
    "php.validate.enable": false,
    "mediaPreview.video.autoPlay": true,
    "vscodeGrok.model": "grok-code-fast",
    "claudeCodeChatUI.api.useCustomAPI": true,
    "claudeCodeChatUI.mcp.enabled": true,
    "inlineChat.enableV2": true,
    "inlineChat.finishOnType": true,
    "inlineChat.accessibleDiffView": "on",
    "chat.tools.terminal.terminalProfile.windows": {},
    "chat.tools.terminal.autoReplyToPrompts": true,
    "chat.mcp.gallery.enabled": true,
    "chat.mcp.discovery.enabled": {
        "cursor-workspace": true,
        "cursor-global": true,
        "windsurf": true,
        "claude-desktop": true
    },
    "chat.edits2.enabled": true,
    "chat.checkpoints.showFileChanges": true,
    "remote.tunnels.tunnelCredentialCommand": null,
    "chat.mcp.serverSampling": {
        "twist3dink.xyz/.vscode/mcp.json: io.github.github/github-mcp-server": {
            "allowedModels": [
                "copilot/gpt-5-mini"
            ]
        }
    },
    "emmet.includeLanguages": {
        "html": "html"
    },
    "emmet.triggerExpansionOnTab": true,
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "files.associations": {
        "*.html": "html",
        "*.css": "css",
        "*.js": "javascript"
    },
    "liveServer.settings.port": 5500,
    "liveServer.settings.root": "/",
    "python.defaultInterpreterPath": "python3",
    "python.linting.enabled": true,
    "python.linting.pylintEnabled": true,
    "python.formatting.provider": "black",
    "prettier.resolveGlobalModules": true,
    "python.globalModuleInstallation": true,
    "npm.exclude": ""
}